<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.serviceprovider">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Service Provider"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity
            android:name="com.example.serviceprovider.Createpassword"
            android:exported="true" />
        <activity
            android:name="com.example.serviceprovider.Demone"
            android:exported="true" />
        <activity
            android:name="com.example.serviceprovider.Login"
            android:exported="true" />
        <activity
            android:name="com.example.serviceprovider.Choice"
            android:exported="true" />
        <activity
            android:name="com.example.serviceprovider.Otpverifi"
            android:exported="true" />
        <activity
            android:name="com.example.serviceprovider.Singup"
            android:exported="true" />
        <activity
            android:name="com.example.serviceprovider.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>